        <s3m:description>Relates a DataInstance to a Data Model Instance (DMI).</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/isPluggableSymbolOf -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/isPluggableSymbolOf">
