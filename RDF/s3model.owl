    <!-- http://www.s3model.com/ns/s3m/s3model/basedOnRM -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/basedOnRM">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/DMI"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/RM"/>
        <s3m:description xml:lang="pt">Relata uma DVI a uma RM.</s3m:description>
        <s3m:description xml:lang="en-us">Relates a DMI to one RM.</s3m:description>
        <s3m:description xml:lang="es">Se relaciona con un adaptador DVI a una RM.</s3m:description>
        <s3m:description xml:lang="ar">يروي DVI إلى RM واحد.</s3m:description>
        <s3m:description xml:lang="hi">एक आर एम के लिए एक डीवीआई संबंधित है।</s3m:description>
        <s3m:description xml:lang="zh-s">涉及一个DVI一个RM。</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/containsCCM -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/containsCCM">
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/RM"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCM"/>
        <s3m:description xml:lang="pt">Define a relação que um MR contém um CCM específica.</s3m:description>
        <s3m:description xml:lang="es">Define la relación que una RM contiene un CCM específica.</s3m:description>
        <s3m:description xml:lang="en-us">Defines the relation that a RM contains a specific CCM.</s3m:description>
        <s3m:description xml:lang="ar">يحدد العلاقة أن RM يحتوي على CCM محددة.</s3m:description>
        <s3m:description xml:lang="hi">संबंध है कि एक आरएम एक विशिष्ट सीसीएम शामिल परिभाषित करता है।</s3m:description>
        <s3m:description xml:lang="zh-s">定义一个RM包含特定CCM的关系。</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/containsPCM -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/containsPCM">
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/DMI"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/PCM"/>
        <s3m:description xml:lang="pt">Define a relação que uma DMI contém um PCM específica.</s3m:description>
        <s3m:description xml:lang="es">Define la relación que un DMI contiene un PCM específico.</s3m:description>
        <s3m:description xml:lang="en-us">Defines the relation that a DMI contains a specific PCM.</s3m:description>
        <s3m:description xml:lang="ar">يحدد العلاقة أن مؤسسة دبي للإعلام يحتوي على PCM معين.</s3m:description>
        <s3m:description xml:lang="hi">संबंध है कि एक डीएमआई एक विशिष्ट पीसीएम शामिल परिभाषित करता है।</s3m:description>
        <s3m:description xml:lang="zh-s">定义一个DMI包含特定PCM的关系。</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/hasException -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/hasException">
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/DataInstanceInvalid"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/Exception"/>
        <s3m:description xml:lang="en">Relates an Exception subclass to an invalid data instance.</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/isConceptSymbolOf -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/isConceptSymbolOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCS"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCM"/>
        <s3m:description xml:lang="pt">Relaciona um símbolo (CSS) para um modelo (CCM).</s3m:description>
        <s3m:description xml:lang="en-us">Relates a symbol (CCS) to a model (CCM).</s3m:description>
        <s3m:description xml:lang="es">Se relaciona con un símbolo (CSS) para un modelo (CCM).</s3m:description>
        <s3m:description xml:lang="ar">يتعلق برمز (CSS) إلى نموذج (CCM).</s3m:description>
        <s3m:description xml:lang="hi">एक मॉडल (सीसीएम) के लिए एक प्रतीक (सीएसएस) से संबंधित है।</s3m:description>
        <s3m:description xml:lang="zh-s">涉及符号（CSS）到模型（CCM）。</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/isInstanceOf -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/isInstanceOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/DataInstance"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/DMI"/>
        <s3m:description xml:lang="pt">Relata uma instância de dados a um Modelo de Dados Instância (DMI).</s3m:description>
        <s3m:description xml:lang="en-us">Relates a DataInstance to a Data Model Instance (DMI).</s3m:description>
        <s3m:description xml:lang="es">Se relaciona de una instancia de datos a un modelo de datos de instancia (DMI).</s3m:description>
        <s3m:description xml:lang="ar">يتصل مثيل البيانات إلى نموذج مثيل البيانات (DMI).</s3m:description>
        <s3m:description xml:lang="hi">डेटा मॉडल उदाहरण (डीएमआई) के लिए एक डाटा उल्लेख संबंधित है।</s3m:description>
        <s3m:description xml:lang="zh-s">涉及数据实例到数据模型实例（DMI）。</s3m:description>
    </owl:ObjectProperty>
    


    <!-- http://www.s3model.com/ns/s3m/s3model/isPluggableSymbolOf -->

    <owl:ObjectProperty rdf:about="http://www.s3model.com/ns/s3m/s3model/isPluggableSymbolOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.s3model.com/ns/s3m/s3model/PCS"/>
        <rdfs:range rdf:resource="http://www.s3model.com/ns/s3m/s3model/PCM"/>
        <s3m:description xml:lang="pt">Refere-se um símbolo (PCS) com um modelo (PCM).</s3m:description>
        <s3m:description xml:lang="en-us">Relates a symbol (PCS) to a model (PCM).</s3m:description>
        <s3m:description xml:lang="es">Se relaciona un símbolo (PCS) a un modelo (PCM).</s3m:description>
        <s3m:description xml:lang="ar">يتعلق برمز (PCS) إلى نموذج (PCM).</s3m:description>
        <s3m:description xml:lang="hi">एक मॉडल (पीसीएम) के लिए एक प्रतीक (पीसीएस) से संबंधित है।</s3m:description>
        <s3m:description xml:lang="zh-s">涉及符号（PCS）到模型（PCM）。</s3m:description>
    </owl:ObjectProperty>
    


