          <xs:documentation xml:lang="en-us">Negative Infinity : Negative infinity of numbers</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#NINFType">
              <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_NINFType.html#NINFType"/>
              <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#ExceptionalValueType"/>
              <rdfs:label xml:lang="en-us">NINFType</rdfs:label>
              <rdfs:comment xml:lang="en-us">Negative Infinity : Negative infinity of numbers</rdfs:comment>
              <rdfs:comment xml:lang="hi">नकारात्मक अनन्तता: नंबर का नकारात्मक अनन्तता</rdfs:comment> 
              <rdfs:comment xml:lang="ar">اللانهاية السالبة: اللانهاية السالبة من الأرقام</rdfs:comment>
              <rdfs:comment xml:lang="es">Infinito negativo: el infinito de números negativos</rdfs:comment>
              <rdfs:comment xml:lang="pt">Infinito negativo: O infinito negativo de números</rdfs:comment>
              <rdfs:comment xml:lang="zh-s">负无穷大：数字的负无穷</rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
          <xs:element fixed="Negative Infinity" maxOccurs="1" minOccurs="1" name="ev-name" type="xs:string"/>
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:element name="NINF" substitutionGroup="s3m:ExceptionalValue" type="s3m:NINFType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#NINF">
          <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#ExceptionalValue"/>
          <s3m:isConceptSymbolOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#NINFType"/>
