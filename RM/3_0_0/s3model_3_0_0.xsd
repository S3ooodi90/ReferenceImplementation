                <rdf:Description rdf:about="hash-function">
                  <rdfs:domain rdf:resource="XdFileType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                  <rdfs:comment xml:lang="en-us">Hash function used to compute hash-result. See: http://en.wikipedia.org/wiki/List_of_hash_functions#Cryptographic_hash_functions</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="alt-txt" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">Text to display in lieu of multimedia display or execution.</xs:documentation>
