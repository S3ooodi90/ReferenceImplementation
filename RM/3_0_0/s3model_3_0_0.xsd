      <xs:restriction base="s3m:ExceptionalValueType">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Positive Infinity : Positive infinity of numbers</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#PINFType">
              <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_PINFType.html#PINFType"/>
              <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#ExceptionalValueType"/>
              <rdfs:label xml:lang="en-us">PINFType</rdfs:label>
              <rdfs:comment xml:lang="en-us">Positive Infinity : Positive infinity of numbers</rdfs:comment>
              <rdfs:comment xml:lang="hi">सकारात्मक अनंत: नंबर के सकारात्मक इन्फिनिटी</rdfs:comment> 
              <rdfs:comment xml:lang="ar">إنفينيتي إيجابي: اللانهاية الإيجابية من الأرقام</rdfs:comment>
              <rdfs:comment xml:lang="es">Infinity positivo: el infinito positivo de los números</rdfs:comment>
              <rdfs:comment xml:lang="pt">Infinito positivo: infinito positivo de números</rdfs:comment>
              <rdfs:comment xml:lang="zh-s">正无穷大：数字正无穷大</rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
          <xs:element fixed="Positive Infinity" maxOccurs="1" minOccurs="1" name="ev-name" type="xs:string"/>
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  
