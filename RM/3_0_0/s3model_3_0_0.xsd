                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="xdfile-language" type="xs:language">
