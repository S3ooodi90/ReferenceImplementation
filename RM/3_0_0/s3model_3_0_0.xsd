                    <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                    <rdfs:comment xml:lang="es"></rdfs:comment>
                    <rdfs:comment xml:lang="pt"></rdfs:comment>
                    <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                  </rdf:Description>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="text-content" type="xs:string">
              <xs:annotation>
                <xs:documentation xml:lang="en-us">The content, if stored locally as text content. The CCM modeler chooses either a uri, a binary-content element or a text-content element.</xs:documentation>
