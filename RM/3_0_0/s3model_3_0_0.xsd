                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:choice maxOccurs="1" minOccurs="1">
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation xml:lang="en-us">URI reference to electronic information stored outside the record as a file, database entry etc, if supplied as a reference. The CCM modeler chooses either a uri, a binary-content element or a text-content element.</xs:documentation>
                <xs:appinfo>
                  <rdf:Description rdf:about="uri">
