<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
  xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" 
  xmlns:s3m="http://www.s3model.com/ns/s3m/"
  targetNamespace="http://www.s3model.com/ns/s3m/" vc:minVersion="1.1">
  <xs:annotation>
    <xs:documentation xml:lang="en-us">This work is Copyright 2016 Data Insights, Inc. All Rights Reserved. 
      This is the Shareable, Structured, Semantic Model (S3Model) reference model (RM) implementation in XML Schema 1.1. 
      The S3Model RM consists of the core datatypes and structures for S3Model domain concept models, called Data Models (DMs). 
      The DMs are composed of Model Components (MCs) that are expressed as constraints on the RM components. 
      For the semantics see the accompanying file s3model.owl. 
      Further documentation and implementation instructions can be found at (or via links from) http://www.datainsights.tech/S3Model 
   </xs:documentation>
    <xs:appinfo>
      <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd">
        <s3m:schemaLocation rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd"/>
        <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/index.html/"/>
        <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model/RM"/>
        <rdfs:label xml:lang="en-us">S3Model Release 3.0.0 Reference Model</rdfs:label>
      </rdf:Description>
    </xs:appinfo>
  </xs:annotation>

  <!--Datatypes package-->

  <xs:complexType abstract="true" name="XdAnyType">
    <xs:annotation>
      <xs:documentation xml:lang="en-us">Serves as the common ancestor of all eXtended data-types (Xd*) in S3Model.</xs:documentation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType">
          <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_XdAnyType.html#XdAnyType"/>
          <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCM"/>
          <rdfs:label xml:lang="en-us">XdAnyType</rdfs:label>
          <rdfs:comment xml:lang="en-us">Serves as a common ancestor of all eXtended data-types (Xd*) in S3Model.</rdfs:comment>
          <rdfs:comment xml:lang="hi"></rdfs:comment> 
          <rdfs:comment xml:lang="ar"></rdfs:comment>
          <rdfs:comment xml:lang="es"></rdfs:comment>
          <rdfs:comment xml:lang="pt"></rdfs:comment>
          <rdfs:comment xml:lang="zh-s"></rdfs:comment>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="label" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Descriptive name of this datatype model. Provides the semantic 'name' for the model. Replaces what would be a global XML element name or class name in conventional modeling.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/label">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
              <rdfs:label xml:lang="en-us">label</rdfs:label>
              <rdfs:comment xml:lang="en-us">Descriptive name of this datatype model. Provides the semantic 'name' for the model. Replaces what would be a global XML element name or class name in conventional modeling.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment> 
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" ref="s3m:ExceptionalValue">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">In an invalid instance, the application can indicate here why data is missing or invalid. The sub-types are based on ISO 21090 NULL Flavors entries, with additions noted from real-world usage.</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element maxOccurs="1" minOccurs="0" name="vtb" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Valid Time Begin. If present this must be a valid datetime including timezone. It is used to indicate the beginning time that information is considered valid.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/vtb">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
              <rdfs:label xml:lang="en-us">vtb</rdfs:label>
              <rdfs:comment xml:lang="en-us">Valid Time Begin. If present this must be a valid datetime including timezone. It is used to indicate the beginning time that information is considered valid.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment> 
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element maxOccurs="1" minOccurs="0" name="vte" type="xs:dateTime" nillable="true">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Valid Time End. If present this must be a valid date-time including timezone. It is used to indicate the ending time that information is considered valid or the time the information expired or will expire.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/vte">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
              <rdfs:label xml:lang="en-us">vte</rdfs:label>
              <rdfs:comment xml:lang="en-us">Valid Time End. If present this must be a valid date-time including timezone. It is used to indicate the ending time that information is considered valid or the time the information expired.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment>
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element maxOccurs="1" minOccurs="0" name="tr" type="xs:dateTimeStamp" nillable="true">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Time Recorded. If present this must be a valid date-time stamp. It is used to indicate the the actual date and time the data was recorded.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/tr">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
              <rdfs:label xml:lang="en-us">tr</rdfs:label>
              <rdfs:comment xml:lang="en-us">Time Recorded. If present this must be a valid date-time stamp. It is used to indicate the the actual date and time the data was recorded.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment>
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      
      <xs:element maxOccurs="1" minOccurs="0" name="modified" type="xs:dateTimeStamp" nillable="true">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Time Modified. If present this must be a valid date-time stamp. It is used to indicate the the actual date and time the data was last changed.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/modified">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
              <rdfs:label xml:lang="en-us">modified</rdfs:label>
              <rdfs:comment xml:lang="en-us">Time Modified. If present this must be a valid date-time stamp. It is used to indicate the the actual date and time the data was last changed.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment>
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      
      <xs:element maxOccurs="1" minOccurs="0" name="latitude" nillable="true">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Latitude in decimal format. Value range -90.000000 to 90.000000.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/latitude">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <rdfs:label xml:lang="en-us">latitude</rdfs:label>
              <rdfs:comment xml:lang="en-us">Latitude in decimal format. Value range -90.000000 to 90.000000.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment> 
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
        <xs:simpleType id="lattype">
          <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="6"/>
            <xs:totalDigits value="8"/>
            <xs:maxInclusive value="90.000000"/>
            <xs:minInclusive value="-90.000000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      
      <xs:element maxOccurs="1" minOccurs="0" name="longitude" nillable="true">
        <xs:annotation>
          <xs:documentation xml:lang="en-us">Longitude in decimal format. Value range -180.000000 to 180.000000.</xs:documentation>
          <xs:appinfo>
            <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType/longitude">
              <rdfs:domain rdf:resource="XdAnyType"/>
              <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <rdfs:label xml:lang="en-us">longitude</rdfs:label>
              <rdfs:comment xml:lang="en-us">Longitude in decimal format. Value range -180.000000 to 180.000000.</rdfs:comment>
              <rdfs:comment xml:lang="hi"></rdfs:comment> 
              <rdfs:comment xml:lang="ar"></rdfs:comment>
              <rdfs:comment xml:lang="es"></rdfs:comment>
              <rdfs:comment xml:lang="pt"></rdfs:comment>
              <rdfs:comment xml:lang="zh-s"></rdfs:comment>
            </rdf:Description>
          </xs:appinfo>
        </xs:annotation>
        <xs:simpleType id="lontype">
          <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="6"/>
            <xs:totalDigits value="9"/>
            <xs:maxInclusive value="180.000000"/>
            <xs:minInclusive value="-180.000000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element abstract="true" name="XdAny" type="s3m:XdAnyType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAny">
          <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCS"/>
          <s3m:isConceptSymbolOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdAnyType"/>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="XdBooleanType">
    <xs:annotation>
      <xs:documentation xml:lang="en-us">Items which represent boolean decisions, such as true/false or yes/no answers. Use for such data, it is important to devise the meanings (usually questions in subjective data) carefully, so that the only allowed results are in fact true or false. The
        possible choices for True or False are enumerations in the CCM. The reference model defines 'true' and 'false' in a choice so only one or the other may be present in the instance data. Potential Misuse: The XdBooleanType should not be used as a replacement for enumerated types such as
        male/female, etc. Such values should be modeled as XdStrings with enumerations and may reference a controlled vocabulary. In any case the enumeration often has more than two values. The elements, 'true' and 'false' are contained in an xs:choice and only one or the other is instantiated in
        the instance data with its value coming from the enumerations defined in a CCM.</xs:documentation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdBooleanType">
          <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_XdBooleanType.html#XdBooleanType"/>
          <rdfs:subClassOf rdf:resource="XdAnyType"/>
          <rdfs:label xml:lang="en-us">XdBooleanType</rdfs:label>
          <rdfs:comment xml:lang="en-us">Items which represent boolean decisions, such as true/false or yes/no answers. Use for such data, it is important to devise the meanings (usually questions in subjective data) carefully, so that the only allowed results are in fact true or false. The
            possible choices for True or False are enumerations in the CCM. The reference model defines 'true' and 'false' in a choice so only one or the other may be present in the instance data. Potential Misuse: The XdBooleanType should not be used as a replacement for enumerated types such as
            male/female, etc. Such values should be modeled as XdStrings with enumerations and may reference a controlled vocabulary. In any case the enumeration often has more than two values. The elements, 'true' and 'false' are contained in an xs:choice and only one or the other is instantiated
            in the instance data with its value coming from the enumerations defined in a CCM.</rdfs:comment>
          <rdfs:comment xml:lang="hi"></rdfs:comment> 
          <rdfs:comment xml:lang="ar"></rdfs:comment>
          <rdfs:comment xml:lang="es"></rdfs:comment>
          <rdfs:comment xml:lang="pt"></rdfs:comment>
          <rdfs:comment xml:lang="zh-s"></rdfs:comment>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="s3m:XdAnyType">
        <xs:choice maxOccurs="1" minOccurs="1">
          <xs:element name="true-value" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">A string that represents a boolean True in the implementation. These are constrained by a set of enumerations in the DM.</xs:documentation>
              <xs:appinfo>
                <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdBooleanType/true-value">
                  <rdfs:domain rdf:resource="XdBooleanType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                  <rdfs:comment xml:lang="en-us">A string that represents a boolean True in the implementation. These are constrained by a set of enumerations in the DM.</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="false-value" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">A string that represents a boolean False in the implementation. These are constrained by a set of enumerations in the DM.</xs:documentation>
              <xs:appinfo>
                <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdBooleanType/false-value">
                  <rdfs:domain rdf:resource="XdBooleanType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                  <rdfs:comment xml:lang="en-us">A string that represents a boolean False in the implementation. These are constrained by a set of enumerations in the DM.</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:choice>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="XdBoolean" substitutionGroup="s3m:XdAny" type="s3m:XdBooleanType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdBoolean">
          <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCS"/>
          <s3m:isConceptSymbolOf rdf:resource="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdBooleanType"/>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="XdLinkType">
    <xs:annotation>
      <xs:documentation xml:lang="en-us">Used to specify a Universal Resource Identifier. Set the pattern facet to accommodate your needs in the DM. Intended use is to provide a mechanism that can be used to link together Concept Models. The relation element allows for the use of a descriptive
        term for the link with an optional URI pointing to the source vocabulary. In most usecases the modeler will define all three of these using the 'fixed' attribute. Other usecases will have the 'relation' and 'relation-uri' elements fixed and the application will provide the 'link-value'.</xs:documentation>
      <xs:appinfo>
        <rdf:Description rdf:about="http://www.s3model.com/ns/s3m/s3model_3_0_0.xsd#XdLinkType">
          <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_XdLinkType.html#XdLinkType"/>
          <rdfs:subClassOf rdf:resource="XdAnyType"/>
          <rdfs:label xml:lang="en-us">XdLinkType</rdfs:label>
          <rdfs:comment xml:lang="en-us">Used to specify a Universal Resource Identifier. Set the pattern facet to accommodate your needs in the DM. Intended use is to provide a mechanism that can be used to link together Concept Models. The relation element allows for the use of a descriptive
            term for the link with an optional URI pointing to the source vocabulary. In most usecases the modeler will define all three of these using the 'fixed' attribute. Other usecases will have the 'relation' and 'relation-uri' elements fixed and the application will provide the 'link-value'.
         </rdfs:comment>
          <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
          <rdfs:comment xml:lang="es"></rdfs:comment>
          <rdfs:comment xml:lang="pt"></rdfs:comment>
          <rdfs:comment xml:lang="zh-s"></rdfs:comment>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="s3m:XdAnyType">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="link" type="xs:anyURI">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">A required URI. A dereferenceable (or not) URL to another CCM or just the ccd-{uuid}.</xs:documentation>
              <xs:appinfo>
                <rdf:Description rdf:about="link">
                  <rdfs:domain rdf:resource="XdLinkType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                  <rdfs:comment xml:lang="en-us">A required URI. A dereferenceable (or not) URL to another CCM or just the ccd-{uuid}.</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="relation" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">A required term, normally drawn from a vocabulary or ontology such as the OBO RO.</xs:documentation>
              <xs:appinfo>
                <rdf:Description rdf:about="relation">
                  <rdfs:domain rdf:resource="XdLinkType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                  <rdfs:comment xml:lang="en-us">A required term, normally drawn from a vocabulary or ontology such as the OBO RO.</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="relation-uri" type="xs:anyURI">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">A URI where the definition of the relation element term can be found. Normally points to an ontology such as the OBO RO http://purl.obolibrary.org/obo/ro.owl</xs:documentation>
              <xs:appinfo>
                <rdf:Description rdf:about="relation-uri">
                  <rdfs:domain rdf:resource="XdLinkType"/>
                  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                  <rdfs:comment xml:lang="en-us">A URI where the definition of the relation element term can be found. Normally points to an ontology such as the OBO RO http://purl.obolibrary.org/obo/ro.owl</rdfs:comment>
                  <rdfs:comment xml:lang="hi"></rdfs:comment> <rdfs:comment xml:lang="ar"></rdfs:comment>
                  <rdfs:comment xml:lang="es"></rdfs:comment>
                  <rdfs:comment xml:lang="pt"></rdfs:comment>
                  <rdfs:comment xml:lang="zh-s"></rdfs:comment>
                </rdf:Description>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="XdLink" substitutionGroup="s3m:XdAny" type="s3m:XdLinkType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:Description rdf:about="XdLink">
          <rdfs:subClassOf rdf:resource="http://www.s3model.com/ns/s3m/s3model/CCS"/>
          <s3m:isConceptSymbolOf rdf:resource="XdLinkType"/>
        </rdf:Description>
      </xs:appinfo>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="XdStringType">
    <xs:annotation>
      <xs:documentation xml:lang="en-us">The string data type can contain characters, line feeds, carriage returns, and tab characters. The use cases are for any free form text entry or for any enumerated lists. Additionally the minimum and maximum lengths may be set and regular expression patterns
        may be specified.</xs:documentation>
      <xs:appinfo>
        <rdf:Description rdf:about="XdStringType">
          <rdfs:isDefinedBy rdf:resource="http://www.s3model.com/docs/3_0_0/s3model_3_0_0_xsd_Complex_Type_s3m_XdStringType.html#XdStringType"/>
          <rdfs:subClassOf rdf:resource="XdAnyType"/>
